@using GMS.Framework.Contract
@using GMS.Framework.Web.Controls
@using GMS.OA.Contract
@using GMS.Web
@model Branch
@{
    Layout = "~/Views/Shared/_Layout.Edit.cshtml";
}
@section PageSpecificStyleSheetIncludes{
}
@section PageSpecificJavascriptIncludes{
    <script type="text/javascript">
   </script>
}
@section MainContent{
<div class="portlet-body form-horizontal form-bordered form-row-stripped">
    <div class="row-fluid">
        <div class="control-group">
            <label class="control-label"><span class="required">*</span>名称：</label>
            <div class="controls">
                <input readonly name="Name" type="text" value="张一易">
            </div>
        </div>

        <div class="control-group">
            <label class="control-label"><span class="required">*</span>制定人/部门：</label>
            <div class="controls">
                <input readonly name="Name" type="text" value="国家">
            </div>
        </div>

        <div class="control-group">
            <label class="control-label"><span class="required">*</span>需求简介：</label>
            <div class="controls">
                <textarea readonly style="margin: 0px; width: 560px; height: 100px;"></textarea>
            </div>
        </div>

        <div class="control-group">
            <label class="control-label"><span class="required">*</span>采购食材：</label>
            <div class="controls">
                <textarea readonly style="margin: 0px; width: 560px; height: 300px;"></textarea>
            </div>
        </div>

        <div class="control-group">
            <label class="control-label"><span class="required">*</span>共需资金：</label>
            <div class="controls">
                <input readonly name="Name" type="text" value="200000.5">
            </div>
        </div>
    </div>
</div>
}

